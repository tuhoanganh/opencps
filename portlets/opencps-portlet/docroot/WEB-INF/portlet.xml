<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>1</portlet-name>
		<display-name>Data Management</display-name>
		<portlet-class>
			org.opencps.datamgt.portlet.DataMamagementPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/portlets/data_management/admin/dictitems.jsp</value>
		</init-param>
		<init-param>
			<name>template-path</name>
			<value>/html/portlets/data_management/admin/</value>
		</init-param>
		<init-param>
			<name>config-template</name>
			<value>/html/portlets/data_management/admin/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content.Language_1</resource-bundle>
		<portlet-info>
			<title>Data Management</title>
			<short-title>Data Management</short-title>
			<keywords>Data Management</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>99</portlet-name>
		<display-name>Sample API</display-name>
		<portlet-class>
			org.opencps.sample.ui.portlet.SamplePortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/portlets/samples/view.jsp</value>
		</init-param>
		<init-param>
			<name>template-path</name>
			<value>/html/portlets/samples/</value>
		</init-param>
		<init-param>
			<name>config-template</name>
			<value>/html/portlets/samples/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content.Language_99</resource-bundle>
		<portlet-info>
			<title>Sample API</title>
			<short-title>99</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>